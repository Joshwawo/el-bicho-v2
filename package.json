{
  "name": "big",
  "version": "1.0.0",
  "description": "Example of music bot with discordx",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:changelog": "npx @discordx/changelog --root=src",
    "dev": "ts-node-esm src/main.ts",
    "start": "node build/main.js",
    "watch": "nodemon --exec ts-node-esm src/main.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanroleplay/discord-music-bot.git"
  },
  "keywords": [
    "discordx",
    "discord.ts",
    "discord.js",
    "bot",
    "discord",
    "typescript"
  ],
  "author": "Vijay Meena <indianoceanroleplay@gmail.com> [https://github.com/oceanroleplay]",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oceanroleplay/discord-music-bot/issues"
  },
  "homepage": "https://github.com/oceanroleplay/discord-music-bot#readme",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "@discordx/importer": "^1.1.10",
    "@discordx/music": "^4.1.0",
    "@discordx/pagination": "^3.3.1",
    "discord.js": "^14.6.0",
    "discordx": "^11.4.0",
    "dotenv": "^16.0.3",
    "ffmpeg-static": "^5.1.0",
    "libsodium-wrappers": "^0.7.10",
    "prism-media": "^1.3.4"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "nodemon": "^2.0.20",
    "prettier": "^2.7.1",
